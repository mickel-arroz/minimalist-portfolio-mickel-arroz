{
  "extends": "astro/tsconfigs/strict",
  // Establecer un Alias. Ahora el @cv es para acceder rapido a cv.json.
  // Establecer un Alias. Ahora el @ es como estar ubicado ya en "src".
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@cv": ["./cv.ts"],
      "@/*": ["src/*"]
    }
  }
}
